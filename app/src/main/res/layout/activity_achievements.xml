<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mapboxGrayLight"
    tools:context=".Achievements">


    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/rate"
        android:layout_above="@+id/ratingBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Rate our App!" />



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/white" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:paddingStart="10dp"
        android:paddingTop="10dp"
        android:text="Achievements"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:paddingTop="10dp"
        android:layout_toEndOf="@+id/textView4"
        app:srcCompat="@drawable/trophy" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="21dp"
        android:text="3/10"
        android:textSize="15sp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="68dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/white" />
    <ImageView
        android:id="@+id/imageView7"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView6"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/white" />
    <ImageView
        android:id="@+id/imageView8"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView7"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/white" />
    <ImageView
        android:id="@+id/imageView9"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView8"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/gray" />
    <ImageView
        android:id="@+id/imageView10"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView9"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/gray" />
    <ImageView
        android:id="@+id/imageView11"
        android:layout_centerHorizontal="true"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView10"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/gray" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="65dp"
        android:layout_height="76dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView6"
        android:layout_marginStart="30dp"
        android:layout_marginTop="2dp"
        app:srcCompat="@drawable/shoe" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="63dp"
        android:layout_height="88dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView7"
        android:layout_marginStart="30dp"
        android:layout_marginTop="4dp"
        app:srcCompat="@drawable/watch" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="66dp"
        android:layout_height="92dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView8"
        android:layout_marginStart="30dp"
        app:srcCompat="@drawable/finish" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="50dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/imageView9"
        android:layout_centerHorizontal="true"
        app:srcCompat="@android:drawable/ic_lock_lock" />
    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="50dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/imageView10"
        android:layout_centerHorizontal="true"
        app:srcCompat="@android:drawable/ic_lock_lock" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="50dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/imageView11"
        android:layout_centerHorizontal="true"
        app:srcCompat="@android:drawable/ic_lock_lock" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:layout_alignTop="@+id/imageView6"
        android:textSize="25sp"
        android:layout_marginStart="130dp"
        android:textStyle="bold|italic"
        android:text="Run a total of 10 kilometers" />
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:layout_alignTop="@+id/imageView7"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        android:layout_marginStart="130dp"
        android:text="Run a total duration of three hours" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:layout_alignTop="@+id/imageView8"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        android:layout_marginStart="130dp"
        android:text="Finished five runs" />


</RelativeLayout>

